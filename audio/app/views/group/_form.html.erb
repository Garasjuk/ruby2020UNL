
  <article class="audio">
    <section class="audio-head">
      <%= simple_form_for :group, html: {multipart: true}, url: group_path do |f| %>
      <p>
        Name <br>
        <%=f.text_field :name %>
      </p>
      <p>
        Description <br>
        <%=f.text_area :description %>
      </p>

      <p>
        Image <br>
        <%=f.file_field :image %>
      </p>

        <p>
        <%=f.submit class: 'btn btn-dark' %>
      </p>
      <% end %>
    </section>
  </article>
