		<div class="container">
			<nav class="navbar navbar-light bg-faded">
				<ul class="navbar-nav">
					<li class="nav-item active">
						<h2>Command page</h2>	
					</li>
				</ul>
				
				<%= form_tag commands_path, class: 'form-inline my-2 my-lg-0', :method => 'get' do %>
					<%= text_field_tag :search, params[:search], placeholder: 'Search', class: 'form-control mr-sm-2' %>
					<%= submit_tag "Search", class: 'btn btn-outline-secondary my-2 my-sm-0', :name => nil %>
				<% end %>
			</nav>
		</div>	
<article class="team">
    <section class="team-head">

        <%= will_paginate @command, :renderer => 'BootstrapPagination::Rails' %>


		<div class="container">
			<div class="row">
				<% @command.each do |command| %>
					<div class="gallery_product col-lg-4 col-md-4 col-sm-4 col-xs-6 filter hdpe">
						<h2><%= link_to command.name, command_path(command) %></h2>

					   
						<br>
						<small>
						  Region: <b><%= command.region %></b> <br>
						  <%= link_to 'Edit', edit_command_path(command), class: 'btn btn-warning' %>
						  <%= link_to 'Destroy', command_path(command), method: :delete, data:{confirm: 'Are you sure?'}, class: 'btn btn-danger' %><br>
						</small>
						<hr>
					</div>
				
				<% end %>     

			</div>
		</div>
		<%= will_paginate @command, :renderer => 'BootstrapPagination::Rails' %>
    </section>
</article>
